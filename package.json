{
  "name": "chaos-validator",
  "version": "1.0.2",
  "description": "Validation Library.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/crysalead-js/chaos-validator.git"
  },
  "keywords": [
    "validation"
  ],
  "scripts": {
    "compile": "BABEL_ENV=production babel -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "mocha --compilers babel:babel-core/register",
    "coverage-report": "babel-node node_modules/isparta/bin/isparta cover _mocha --i \"src/**/*.js\"",
    "push-coverage": "cat ./coverage/lcov.info | coveralls",
    "cleanup": "rm -rf ./coverage"
  },
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "dependencies": {
    "co": "^4.6.0",
    "date-format": "0.0.2",
    "extend-merge": "^1.0.3",
    "string-placeholder": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "bluebird": "^3.4.1",
    "chai-kahlan": "^1.0.4",
    "coveralls": "^2.11.9",
    "isparta": "^4.0.0",
    "jasmine-kahlan": "^1.0.2",
    "mocha": "^2.5.3"
  },
  "babel": {
    "env": {
      "development": {
        "plugins": ["transform-es2015-modules-commonjs"]
      },
      "production": {
        "presets": ["es2015"]
      }
    }
  }
}
